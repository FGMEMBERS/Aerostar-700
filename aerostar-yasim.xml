<fuselage ax="1.25"  ay="-0.45" az="-0.412" bx="-2.1" by="-0.45" bz="-0.412" 
width="0.5" taper="1" midpoint="0"/>

<fuselage ax="1.25"  ay="0.45" az="-0.412" bx="-2.1" by="0.45" bz="-0.412" 
width="0.5" taper="1" midpoint="0"/>

<wing x="-1.6" y="0.65" z="0.05" length="2.94" chord="2.18" 
sweep="-3.7" dihedral="2" taper="0.348" camber="0.0" incidence="1.0">
    <stall aoa="15" width="18" peak="1.5"/>
    <flap0 start="0.1" end=".5" lift="1.4" drag="1.6"/>
    <flap1 start=".5" end=".9" lift="1.2" drag="1.1"/>
    <control-input axis="/controls/flight/flaps" control="FLAP0"/>
    <control-speed control="FLAP0" transition-time="10"/>
    <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>
    <control-input axis="/controls/flight/aileron" control="FLAP1" split="true"/>
    <control-input axis="/controls/flight/aileron-trim" control="FLAP1" split="true"
    src0="-1.0" dst0="-0.5" src1="1.0" dst1="0.5"/>
    <control-output control="FLAP1" side="left" prop="/surface-positions/left-aileron-pos-norm"/>
    <control-output control="FLAP1" side="right" prop="/surface-positions/right-aileron-pos-norm"/>
</wing>

<hstab x="-5.85" y="0.237" z="0.61" length="1.8" chord="1.2"
sweep="27" dihedral="0" taper="0.6" camber = "0">
  <stall aoa="15" width="3" peak="1.3"/>
  <flap0 start="0" end="1" lift="1.6" drag="1.2"/>
  <control-input axis="/controls/flight/elevator" control="FLAP0"/>
  <control-input axis="/controls/flight/elevator-trim" control="FLAP0"
  src0="-1.0" dst0="-0.5" src1="1.0" dst1="0.5"/>
  <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
</hstab>

<vstab x="-5.12" y="0.00" z="0.83" length="2.18" chord="2.4"
    sweep="30" taper="0.25">
  <stall aoa="16" width="4" peak="1.5"/>
  <flap0 start="0" end="1" lift="1.4" drag="1.2"/>
  <control-input axis="/controls/flight/rudder" control="FLAP0" invert="true"/>
  <control-input axis="/controls/flight/rudder-trim" control="FLAP0" 
  src0="-1.0" dst0="0.5" src1="1.0" dst1="-0.5"/>
  <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"/>
</vstab>

<mstab x="1.668" y="0.00" z="0" length="0.01" chord="0.01">
  <stall aoa="18" width="4" peak="1.5"/>
  <flap0 start="0" end="1" lift="0" drag="0"/>
  <control-input axis="controls/electric/steering-toggle" control="FLAP0"/>
  <control-output control="FLAP0" prop="/controls/gear/steering"/>
</mstab>


