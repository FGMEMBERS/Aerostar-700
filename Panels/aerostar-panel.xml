     <w>20</w>
     <h>30</h>
     <condition>
       <less-than>
         <property>controls/engines/engine[0]/magnetos</property>
         <value>3</value>
       </less-than>
     </condition>
     <binding>
       <command>property-adjust</command>
       <property>controls/engines/engine[0]/magnetos</property>
       <step>1</step>
       <min>0</min>
       <max>4</max>
       <wrap>false</wrap>
