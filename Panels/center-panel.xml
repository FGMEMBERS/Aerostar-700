<?xml version="1.0"?>
<PropertyList>

 <name>Center Panel</name>
 <background>Aircraft/Aerostar-700/Panels/transparent-bg.rgb</background>
 <w>256</w>
 <h>512</h>

<instruments>

<instrument>
  <name>centerpanel hotspots</name>
  <x>128</x>
  <y>256</y>
  <w>256</w>
  <h>512</h>
  <w-base>256</w-base>
  <h-base>512</h-base>

 <actions> 
   <!---            FD Controller -->
   
    <action>
     <name>HDG mode</name>
     <button>0</button>
     <x>-20</x>
     <y>-27</y>
     <w>9</w>
     <h>7</h>
     <binding>
       <command>nasal</command>
       <script> kfcmode = getprop ("instrumentation/kfc200/fdmode");
         if ( kfcmode  != "hdg"){setprop("instrumentation/kfc200/fdmode","hdg");
         }else{
         setprop("instrumentation/kfc200/fdmode","off");
         }</script>
     </binding>
   </action>
   
   <action>
     <name>FD mode</name>
     <button>0</button>
     <x>-8</x>
     <y>-27</y>
     <w>9</w>
     <h>7</h>
     <binding>
       <command>property-toggle</command>
       <property>/instrumentation/kfc200/fd_on</property>
     </binding>
   </action>
   
   <action>
     <name>ALT mode</name>
     <button>0</button>
     <x>4</x>
     <y>-27</y>
     <w>9</w>
     <h>7</h>
     <binding>
       <command>nasal</command>
       <script>kfcmode = getprop ("instrumentation/kfc200/fdmodeV");
         if ( kfcmode != "alt-arm"){setprop("instrumentation/kfc200/fdmodeV","alt-arm");
         }else{
         setprop("instrumentation/kfc200/fdmodeV","off");
         }</script>
     </binding>
   </action>
   

   
   <action>
     <name>NAV mode</name>
     <button>0</button>
     <x>-20</x>
     <y>-43</y>
     <w>9</w>
     <h>7</h>
     <binding>
       <command>nasal</command>
       <script>kfcmode = getprop ("instrumentation/kfc200/fdmode");
         if ( kfcmode  != "nav-arm"){setprop("instrumentation/kfc200/fdmode","nav-arm");
         }else{if(kfcmode == "nav-cpld"){setprop("instrumentation/kfc200/fdmode","off");}
         }</script>
     </binding>
   </action>
   
   <action>
     <name>BC mode</name>
     <button>0</button>
     <x>-9</x>
     <y>-43</y>
     <w>9</w>
     <h>7</h>
     <binding>
       <command>nasal</command>
       <script> kfcmode = getprop ("instrumentation/kfc200/fdmode");
         if ( kfcmode  != "bc"){setprop("instrumentation/kfc200/fdmode","bc");
         }else{
         setprop("instrumentation/kfc200/fdmode","off");
         }</script>
     </binding>
   </action>
   
   <action>
     <name>APPR mode</name>
     <button>0</button>
     <x>3</x>
     <y>-43</y>
     <w>9</w>
     <h>7</h>
     <binding>
       <command>nasal</command>
       <script> kfcmode = getprop ("instrumentation/kfc200/fdmode");
         if ( kfcmode  != "appr"){setprop("instrumentation/kfc200/fdmode","appr");
         }else{
         setprop("instrumentation/kfc200/fdmode","off");
         }</script>
     </binding>
   </action>

   <action>
     <name>AP-ON mode</name>
     <button>0</button>
     <x>18</x>
     <y>-35</y>
     <w>10</w>
     <h>10</h>
     <binding>
       <command>property-toggle</command>
       <property>/autopilot/locks/passive-mode</property>
     </binding>
   </action>

   	<action>
    	<name>Trim-Down</name>
     	<button>0</button>
     	<x>-35</x>
     	<y>-30</y>
     	<w>10</w>
     	<h>10</h>
   			<condition>
				<not-equals>
					<property>/autopilot/locks/altitude</property>
					<value></value>
				</not-equals>
			</condition>
     	<binding>
       			<command>property-adjust</command>
       			<property>/autopilot/settings/target-pitch-deg</property>
       			<step>-0.05</step>
       			<min>-15.0</min>
       			<max>15.0</max>
       			<wrap>false</wrap>
     	</binding>
   	</action>

   <action>
     <name>Trim-Up</name>
     <button>0</button>
     <x>-35</x>
     <y>-40</y>
     <w>10</w>
     <h>10</h>
		<condition>
			<not-equals>
				<property>/autopilot/locks/altitude</property>
				<value></value>
			</not-equals>
		</condition>
		<binding>
   			<command>property-adjust</command>
       			<property>/autopilot/settings/target-pitch-deg</property>
       			<step>0.05</step>
       			<min>-15.0</min>
       			<max>15.0</max>
       			<wrap>false</wrap>
     	</binding>
   </action>

