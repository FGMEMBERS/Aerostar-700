<?xml version="1.0"?>

<PropertyList>

<name>Center Panel</name>
<background>Aircraft/Aerostar-700/Panels/transparent-bg.rgb</background>
<w>256</w>
<h>512</h>

    <instruments>

        <instrument>
            <name>centerpanel hotspots</name>
            <x>128</x>
            <y>256</y>
            <w>256</w>
            <h>512</h>
            <w-base>256</w-base>
            <h-base>512</h-base>

            <actions>

            <action>
                <name>HDG mode</name>
                <button>0</button>
                <x>-20</x>
                <y>-27</y>
                <w>9</w>
                <h>7</h>
                <binding>
                    <command>nasal</command>
                    <script>aerostar.FD.set_mode("HDG");</script>
                </binding>
            </action>

            <action>
                <name>FD mode</name>
                <button>0</button>
                <x>-8</x>
                <y>-27</y>
                <w>9</w>
                <h>7</h>
                <binding>
                    <command>nasal</command>
                    <script>aerostar.FD.set_mode("FD");</script>
                </binding>
            </action>

            <action>
                <name>ALT mode</name>
                <button>0</button>
                <x>4</x>
                <y>-27</y>
                <w>9</w>
                <h>7</h>
                <binding>
                    <command>nasal</command>
                    <script>aerostar.FD.set_mode("ALT");</script>
                </binding>
            </action>

            <action>
                <name>NAV mode</name>
                <button>0</button>
                <x>-20</x>
                <y>-43</y>
                <w>9</w>
                <h>7</h>
                <binding>
                    <command>nasal</command>
                    <script>aerostar.FD.set_mode("NAV");</script>
                </binding>
            </action>

            <action>
                <name>BC mode</name>
                <button>0</button>
                <x>-9</x>
                <y>-43</y>
                <w>9</w>
                <h>7</h>
                <binding>
                    <command>nasal</command>
                    <script>aerostar.FD.set_mode("BC");</script>
                </binding>
            </action>

            <action>
                <name>APPR mode</name>
                <button>0</button>
                <x>3</x>
                <y>-43</y>
                <w>9</w>
                <h>7</h>
                <binding>
                    <command>nasal</command>
                    <script>aerostar.FD.set_mode("APPR");</script>
                </binding>
            </action>

            <action>
                <name>AP-ON mode</name>
                <button>0</button>
                <x>18</x>
                <y>-35</y>
                <w>10</w>
                <h>10</h>
                <binding>
                    <command>property-toggle</command>
                    <property>autopilot/locks/passive-mode</property>
                </binding>
            </action>

            <action>
                <name>Pitch Trim-Down</name>
                <button>0</button>
                <x>-35</x>
                <y>-30</y>
                <w>10</w>
                <h>10</h>
                <binding>
                    <script>aerostar.FD.pitch_wheel(-1);</script>
                </binding>
            </action>

            <action>
                <name>Pitch Trim-Up</name>
                <button>0</button>
                <x>-35</x>
                <y>-40</y>
                <w>10</w>
                <h>10</h>
                <binding>
                    <script>aerostar.FD.pitch_wheel(1);</script>
                </binding>
            </action>

<!-- ############## THROTTLE QUADRANT ##############-->

<!--           throttles           -->
<action>
  <name>Left throttle increase</name>
  <button>0</button>
  <x>-48</x>
  <y>68</y>
  <w>14</w>
  <h>10</h>
  <binding>
    <command>property-adjust</command>
    <property>/controls/engines/engine[0]/throttle</property>
    <step>0.05</step>
    <min>0.0</min>
    <max>1.0</max>
    <wrap>false</wrap>
  </binding>
</action>

