<?xml version="1.0"?>
<!-- 
Airspeed Indicator (260 kt)
Syd Adams
texture by tigert
-->
<PropertyList>

    <path>asi260.ac</path>
    
    <animation>
        <type>material</type>
        <object-name>ASIneedle</object-name>
        <object-name>ASIface</object-name>
        <emission>
            <red>0.8</red>
            <green>0.4</green>
            <blue>0.2</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ASIneedle</object-name>
        <property>velocities/airspeed-kt</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>40</ind><dep>119</dep></entry>
            <entry><ind>60</ind><dep>39</dep></entry>
            <entry><ind>80</ind><dep>64</dep></entry>
            <entry><ind>180</ind><dep>239</dep></entry>
            <entry><ind>260</ind><dep>339</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
        </axis>
    </animation>

</PropertyList>