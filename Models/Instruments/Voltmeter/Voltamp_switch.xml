<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

 <path>Voltamp_switch.ac</path>

  <animation>
    <type>material</type>
	<object-name>switch</object-name>
  </animation>
  
	<animation>
    	<type>rotate</type>
    	<object-name>switch</object-name>
			<property>controls/electric/ammeter-switch</property>
    		<factor>50</factor>
    	<center>
      		<x-m> -0.002 </x-m>
      		<y-m> 0 </y-m>
      		<z-m> 0 </z-m>
    	</center>
    	<axis>
     		<x>0</x>
      		<y>0</y>
      		<z>1</z>
    	</axis>
  	</animation>

  <animation>
    <type>pick</type>
    <object-name>switch</object-name>
	<action>
		<button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>controls/electric/ammeter-switch</property>
            </binding>
        </action>
    </animation>


</PropertyList>


