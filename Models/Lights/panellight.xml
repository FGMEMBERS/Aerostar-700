<?xml version="1.0" encoding="UTF-8" ?>
<!--Clm76 - 2015-->


<PropertyList>
	<path>panellight.ac</path>

	<animation>
		<type>select</type>
		<condition>
			<and>
				<greater-than>
					<property>systems/electrical/volts</property>			
					<value>12</value>
				</greater-than>
				<property>/sim/rendering/rembrandt/enabled</property>
			</and>
		</condition>
	</animation>

	<animation>
		<type>light</type>
		<light-type>spot</light-type>
		<object-name>W_Panel</object-name>
		<nopreview/>
		<position>
			<x>0</x>
			<y>0</y>
			<z>0.4</z>
		</position>
		<direction>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</direction>
		<ambient>
			<r>1</r>
			<g>1</g>
			<b>1</b>
			<a>1.0</a>
		</ambient>
		<diffuse>
			<r>1</r>
			<g>1</g>
			<b>1</b>
			<a>1.0</a>
		</diffuse>
		<specular>
			<r>1</r>
			<g>1</g>
			<b>1</b>
			<a>1.0</a>
		</specular>
		<dim-factor>
			<property>controls/lighting/panel-white</property>
		</dim-factor>
		<attenuation>
			<c>5</c>
			<l>0.1</l>
			<q>0.05</q>
		</attenuation>
		<exponent>0.1</exponent>
		<near-m>0.2</near-m>
		<far-m>1.1</far-m>
		<cutoff>32</cutoff>	
	</animation>

	<animation>
		<type>light</type>
		<light-type>spot</light-type>
		<object-name>R_Panel</object-name>
		<nopreview/>
		<position>
			<x>0</x>
			<y>0</y>
			<z>0.4</z>
		</position>
		<direction>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</direction>
		<ambient>
			<r>1</r>
			<g>0</g>
			<b>0</b>
			<a>1.0</a>
		</ambient>
		<diffuse>
			<r>1</r>
			<g>0</g>
			<b>0</b>
			<a>1.0</a>
		</diffuse>
		<specular>
			<r>1</r>
			<g>0</g>
			<b>0</b>
			<a>1.0</a>
		</specular>
		<dim-factor>
			<property>controls/lighting/panel-red</property>
		</dim-factor>
		<attenuation>
			<c>5</c>
			<l>0.1</l>
			<q>0.05</q>
		</attenuation>
		<exponent>0.1</exponent>
		<near-m>0.2</near-m>
		<far-m>1.1</far-m>
		<cutoff>32</cutoff>	
	</animation>

</PropertyList>
