 <animation>
	<type>shader</type>
        <shader>chrome</shader>
	<texture>Aircraft/Aerostar-700/Models/chrome1.rgb</texture>
	<object-name>Chrome</object-name>
</animation>

	<animation>
  		<type>material</type>
  		<object-name>gearon.1</object-name>
  		<condition>
  			<greater-than>
  				<property>systems/electrical/volts</property>
  				<value>1.0</value>
  			</greater-than>
  		</condition>
  		<emission>
    		<red>0.1</red>
    		<green>0.9</green>
    		<blue>0.4</blue>
    		<factor-prop>/gear/gear[0]/gear-down-locked</factor-prop>
    	</emission>
	</animation>
	
