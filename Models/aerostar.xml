 <animation>
   <type>noshadow</type>
   <object-name>LHpropblur</object-name>
   <object-name>LHpropdisc</object-name>
   <object-name>RHpropblur</object-name>
   <object-name>RHpropdisc</object-name>
 </animation>

 
 <animation>
   <type>select</type>
   <object-name>RightWheel</object-name>
   <object-name>LeftWheel</object-name>
   <object-name>NoseWheel</object-name>
   <condition>
     <equals>
       <property>/gear/gear[0]/position-norm</property>
       <value>1.0</value>
     </equals>
   </condition>
 </animation>


 <animation>
   <type>select</type>
   <object-name>LHprop</object-name>
   <condition>
     <less-than>
       <property>/engines/engine[0]/rpm</property>
       <value>400</value>
     </less-than>
   </condition>
 </animation>
 
 <animation>
   <type>select</type>
   <object-name>LHpropblur</object-name>
   <condition>
     <greater-than>
       <property>/engines/engine[0]/rpm</property>
       <value>399</value>
     </greater-than>
