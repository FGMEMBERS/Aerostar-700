    <animation>
        <type>select</type>
        <object-name>redglow</object-name>
        <object-name>greenglow</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>sim/multiplay/generic/int[2]</property>
                    <value>0.2</value>
                </greater-than>
                <equals>
                    <property>sim/multiplay/generic/int[0]</property>
                    <value>0</value>
                </equals>
            </and>
        </condition>
    </animation>

    <animation>
        <type>flash</type>
        <object-name>greenglow</object-name>
        <center>
            <x-m>1.096</x-m>
            <y-m>5.320</y-m>
            <z-m>0.259</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
