<?xml version="1.0"?>

<PropertyList>

	<fx>
<!-- Engine 1 -->
	<crank>
   		<name>engstart1</name>
   		<mode>once</mode>
   		<path>Sounds/coughing.wav</path>
   		<property>engines/engine[0]/running</property>
   		<reference-dist>5.0</reference-dist>
   		<max-dist>10.0</max-dist>
   		<position>
    		<x>0.5</x>
    		<y>1.8</y>
    		<z>0.2</z>
   		</position>
   		<volume>
    		<property>sim/sound/A700/Ovolume</property>
    	</volume>
  	</crank>

  	<crank>
   		<name>crank1</name>
   		<mode>looped</mode>
   		<path>Sounds/cranking.wav</path>
   		<condition>
    		<property>engines/engine[0]/cranking</property>
    		<not>
     			<property>engines/engine[0]/running</property>
    		</not>
   		</condition>
   		<reference-dist>5.0</reference-dist>
   		<max-dist>10.0</max-dist>
   		<position>
    		<x>0.5</x>
    		<y>1.8</y>
    		<z>0.2</z>
   		</position>
   		<volume>
    		<property>sim/sound/A700/Ovolume</property>
    	</volume>
  	</crank>

	<engine>
    	<name>engine1</name>
    	<mode>looped</mode>
    	<path>Sounds/wasp.wav</path>
    	<condition>
      		<equals>
        		<property>engines/engine[0]/running</property>
        		<value>1</value>
      		</equals>
    	</condition>
   		<reference-dist>100.0</reference-dist>
   		<max-dist>500.0</max-dist>
    	<position>
    		<x>0.5</x>
    		<y>1.8</y>
    		<z>0.2</z>
   		</position>
    	<volume>
      		<property>engines/engine[0]/thrust-lbs</property>
      		<factor>0.0015</factor>
      		<min>0.2</min>
      		<max>1.0</max>
    	</volume>
   		<volume>
    		<property>sim/sound/A700/Ovolume</property>
   		</volume>
    	<pitch>
      		<property>engines/engine[0]/thrust-lbs</property>
      		<factor>0.0015</factor>
      		<min>0.4</min>
      		<max>1.0</max>
      		<offset> 0.0</offset>
    	</pitch>
  	</engine>

<!-- Engine 2 -->

	<crank>
   		<name>engstart2</name>
   		<path>Sounds/coughing.wav</path>
   		<property>engines/engine[1]/running</property>
   		<reference-dist>5.0</reference-dist>
   		<max-dist>10.0</max-dist>
   		<position>
    		<x>0.5</x>
    		<y>-1.8</y>
    		<z>0.2</z>
   		</position>
   		<volume>
    		<property>sim/sound/A700/Ovolume</property>
   		</volume>
  	</crank>

  	<crank>
   		<name>crank2</name>
   		<mode>looped</mode>
   		<path>Sounds/cranking.wav</path>
   		<condition>
    		<property>engines/engine[1]/cranking</property>
    		<not>
     			<property>engines/engine[1]/running</property>
    		</not>
   		</condition>
   		<reference-dist>5.0</reference-dist>
   		<max-dist>10.0</max-dist>
   		<position>
    		<x>0.5</x>
    		<y>-1.8</y>
    		<z>0.2</z>
   		</position>
   		<volume>
    		<property>sim/sound/A700/Ovolume</property>
   		</volume>
  	</crank>

	<engine>
    	<name>engine2</name>
    	<mode>looped</mode>
    	<path>Sounds/wasp.wav</path>
    	<condition>
      		<equals>
        		<property>engines/engine[1]/running</property>
        		<value>1</value>
      		</equals>
    	</condition>
   		<reference-dist>100.0</reference-dist>
   		<max-dist>500.0</max-dist>
    	<position>
    		<x>0.5</x>
    		<y>-1.8</y>
    		<z>0.2</z>
   		</position>
    	<volume>
      		<property>engines/engine[1]/thrust-lbs</property>
      		<factor>0.0015</factor>
      		<min>0.2</min>
      		<max>1.0</max>
    	</volume>
   		<volume>
    		<property>sim/sound/A700/Ovolume</property>
   		</volume>
    	<pitch>
      		<property>engines/engine[1]/thrust-lbs</property>
      		<factor>0.0015</factor>
      		<min>0.4</min>
      		<max>1.0</max>
      		<offset> 0.0</offset>
    	</pitch>
  	</engine>

  
	<rumble>
   		<name>rumble</name>
   		<mode>looped</mode>
   		<path>Sounds/rumble.wav</path>
        <position>
    		<x>0.0</x>
    		<y>0.0</y>
    		<z>-0.5</z>
   		</position>
   		<condition>
    		<or>
     			<property>gear/gear[0]/wow</property>
     			<property>gear/gear[1]/wow</property>
     			<property>gear/gear[2]/wow</property>
    		</or>
   		</condition>
   		<reference-dist>1.0</reference-dist>
   		<max-dist>5.0</max-dist>
   		<volume>
    		<property>sim/sound/A700/Cvolume</property>
   		</volume>
   		<volume>
    		<property>velocities/groundspeed-kt</property>
    		<factor>0.05</factor>
   		</volume>
  	</rumble>

  	<gear>
   		<name>gear</name>
   		<mode>in-transit</mode>
   		<path>Sounds/gear.wav</path>
   		<property>gear/gear[0]/position-norm</property>
        <position>
    		<x>0.0</x>
    		<y>0.0</y>
    		<z>-0.5</z>
   		</position>
   		<volume>
    		<property>sim/sound/A700/Cvolume</property>
    	</volume>
   		<pitch>
    		<internal>dt_play</internal>
    		<offset>0.8</offset>
    		<factor>-0.001</factor>
   		</pitch>
   		<reference-dist>10.0</reference-dist>
   		<max-dist>20.0</max-dist>
  	</gear>
 
	<gear>
   		<name>gear-lock</name>
   		<path>Sounds/gear-lck.wav</path>
   		<condition>
    		<equals>
     			<property>gear/gear[0]/position-norm</property>
     			<value>0</value>
    		</equals>
   		</condition>
        <position>
    		<x>0.0</x>
    		<y>0.0</y>
    		<z>-0.5</z>
   		</position>
   		<volume>
    		<property>sim/sound/A700/Cvolume</property>
    	</volume>
   		<reference-dist>10.0</reference-dist>
   		<max-dist>20.0</max-dist>
  	</gear>

	<squeal>
   		<name>squeal</name>
   		<path>Sounds/squeal.wav</path>
   		<reference-dist>20.0</reference-dist>
   		<max-dist>50</max-dist>
   		<condition>
    		<or>
     			<property>gear/gear[0]/wow</property>
     			<property>gear/gear[1]/wow</property>
     			<property>gear/gear[2]/wow</property>
    		</or>
   		</condition>
        <position>
    		<x>0.0</x>
    		<y>0.0</y>
    		<z>-0.5</z>
   		</position>
   		<volume>
    		<property>sim/sound/A700/Ovolume</property>
   		</volume>
   		<volume>
    		<property>velocities/airspeed-kt</property>
    		<factor>0.01</factor>
   		</volume>
   		<volume>
    		<internal>dt_stop</internal>
    		<factor>0.05</factor>
    		<max>1.0</max>
   		</volume>
   		<pitch>
    		<property>velocities/airspeed-kt</property>
    		<factor>0.0025</factor>
    		<offset>1.2</offset>
   		</pitch>
  	</squeal>
 
  	<flap>
   		<name>flaps</name>
   		<mode>in-transit</mode>
   		<path>Sounds/flaps.wav</path>
   		<property>surface-positions/flap-pos-norm</property>
        <position>
    		<x>-0.3</x>
    		<y>0.0</y>
    		<z>-0.0</z>
   		</position>
   		<volume>
    		<factor>0.45</factor>
   		</volume>
   		<reference-dist>1.5</reference-dist>
   		<max-dist>4.0</max-dist>
  	</flap>


	<stall>
   		<name>stall</name>
   		<path>Sounds/stall.wav</path>
   		<property>sim/alarms/stall-warning</property>
   		<volume>
    		<property>velocities/airspeed-kt</property>
    		<min>30.0</min>
   		</volume>
   		<reference-dist>2.0</reference-dist>
   		<max-dist>4.0</max-dist>
  	</stall>
 
	<wind>
   		<name>wind</name>
   		<mode>looped</mode>
   		<path>Sounds/wind.wav</path>
   		<reference-dist>2.0</reference-dist>
   		<max-dist>5.0</max-dist>
   		<property>/velocities/airspeed-kt</property>
        <position>
    		<x>-0.8</x>
    		<y>0.0</y>
    		<z>0.5</z>
   		</position>
   		<volume>
    		<property>/position/altitude-ft</property>
    		<factor>-0.000015</factor>
    		<offset>1.0</offset>
    		<min>0.1</min>
    		<max>0.5</max>
   		</volume>
   		<volume>
    		<property>/velocities/airspeed-kt</property>
    		<factor>0.0015</factor>
    		<min>0.03</min>
    		<max>0.5</max>
   		</volume>
   		<volume>
    		<property>sim/sound/A700/Cvolume</property>
   		</volume>
   		<pitch>
    		<property>/velocities/airspeed-kt</property>
    		<factor>0.01</factor>
    		<offset>0.8</offset>
   		</pitch>
	</wind>

	</fx>
 
</PropertyList>